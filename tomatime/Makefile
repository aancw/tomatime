compiler_yacc_impl_clean:
compiler_lex_make_all:
compiler_lex_clean:
compiler_clean: compiler_rcc_clean compiler_moc_header_clean compiler_uic_clean 

####### Compile

main.o: main.cpp tomatime.h \
		settings.h \
		about.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o main.cpp

tomatime.o: tomatime.cpp tomatime.h \
		settings.h \
		about.h \
		ui_tomatime.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o tomatime.o tomatime.cpp

settings.o: settings.cpp settings.h \
		tomatime.h \
		about.h \
		ui_settings.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o settings.o settings.cpp

about.o: about.cpp about.h \
		ui_about.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o about.o about.cpp

qrc_tomatime.o: qrc_tomatime.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o qrc_tomatime.o qrc_tomatime.cpp

moc_tomatime.o: moc_tomatime.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_tomatime.o moc_tomatime.cpp

moc_settings.o: moc_settings.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_settings.o moc_settings.cpp

moc_about.o: moc_about.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_about.o moc_about.cpp

####### Install

install:  FORCE
	install -m 751 $(TARGET) $(BINDIR)
	install -m 644 $(TARGET).desktop /usr/share/applications/

	for i in 1024 128 16 24 256 32 48 512 64; do \
		install -D -m 0644 img/icons/$$i/$(TARGET).png \
												/usr/share/icons/hicolor/$${i}x$${i}/$(TARGET).png ; \
	done

uninstall:  FORCE
	rm -f $(BINDIR)/$(TARGET)
	rm -f /usr/share/applications/$(TARGET).desktop

	for i in 1024 128 16 24 256 32 48 512 64; do \
		rm -f /usr/share/icons/hicolor/$${i}x$${i}/$(TARGET).png ; \
	done

FORCE:

